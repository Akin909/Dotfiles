{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            "off",
            "unix"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": "off"
    },
    "globals": {},
    "env": {
        "es6": true,
        "browser": true,
        "jasmine": true,
        "mocha": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "classes": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "no-console": 0
    },
    "globals": {
        "_": false,
        "remote": false,
        "process": false,
        "module": false,
        "require": false,
        "__dirname": false,
        "preloadedData": false
    },
    "root": true
}
