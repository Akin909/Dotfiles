{
    "rules": {
        "indent":0,
        //"indent": [ "warn" , 4],
        "linebreak-style": [ "off", "unix" ],
        "quotes": [ "error", "single" ],
        "semi": [ "error", "always" ],
        "no-console": "off",
        "strict": 0,
        "no-unused-vars": 0,
        "no-undef":0,
        "no-undefined":0,
        "camelcase": 1,
        "no-underscore-dangle": 0,
        "no-console": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "jasmine": true,
        "mocha": true
    },
    "settings": {
        "ecmascript": 6,
        "jsx": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "classes": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "globals": {
        "_": false,
        "remote": false,
        "process": false,
        "module": false,
        "require": false,
        "__dirname": false,
        "preloadedData": false
    },
    "root": true
}
