{
  "typescript.suggestionActions.enabled": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.colorSupport": true,
  "list.normalMappings": {
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "d": "expr:GetDeleteAction"
  },
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-w>": "command:wincmd k"
  },
  "suggest.noselect": false,
  "suggest.detailField": "menu",
  "javascript.referencesCodeLens.enable": true,
  "codeLens.enable": true,
  "codeLens.separator": "‣",
  "suggest.fixInsertedWord": true,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextLines": 3,
  "coc.preferences.hoverTarget": "float",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualTextPrefix": "ﯦ ",
  "diagnostic.errorSign": "✗",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ﯦ",
  "signature.target": "float",
  "signature.preferShownAbove": true,
  "signature.maxWindowHeight": 4,
  "coc.preferences.snippetStatusText": "",
  "coc.preferences.previewAutoClose": true,
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "rei": ["reason"]
  }
}
