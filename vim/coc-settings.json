{
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "typescript.suggestionActions.enabled": true,
  "tsserver.enableJavascript": false,
  "emmet.showAbbreviationSuggestions": true,
  "coc.preferences.noselect": true,
  "coc.preferences.diagnostic.virtualText": true,
  "coc.preferences.codeLens.enable": true,
  "coc.preferences.echodocSupport": true,
  "list.normalMappings": {
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "d": "expr:GetDeleteAction"
  },
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-w>": "command:wincmd k"
  },
  "coc.preferences.acceptSuggestionOnCommitCharacter": false,
  "coc.preferences.diagnostic.locationlist": true,
  "coc.preferences.diagnostic.virtualTextPrefix": "ﯦ ",
  "coc.preferences.diagnostic.errorSign": "",
  "coc.preferences.diagnostic.warningSign": "",
  "coc.preferences.diagnostic.infoSign": "",
  "coc.preferences.diagnostic.hintSign": "ﯦ",
  "coc.preferences.snippetIndicator": " ",
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "rei": ["reason"]
  }
}
